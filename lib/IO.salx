import "../lib/System.salx";
import "../lib/CStr.salx";

namespace io {
    void input(i8* str,i64 len){
        sys::read(0,str,len);
    }
    void print(i8* str){
        i64 len = cstr::len(str);
        sys::write(1,str,len);
    }
    void error(i8* str){
        i64 len = cstr::len(str);
        sys::write(2,str,len);
    }
}